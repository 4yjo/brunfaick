{
  "name": "brunfaick",
  "version": "0.3.1",
  "description": "A Brainfuck interpreter implemented in JavaScript.",
  "license": "MIT",
  "repository": "maxhallinan/brunfaick",
  "author": {
    "name": "Max Hallinan",
    "email": "maxhallinan@riseup.net",
    "url": "github.com/maxhallinan"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "xo && mocha test/**/* --timeout=40000",
    "test-dev": "mocha --watch test/unit/**/*"
  },
  "files": [
    "index.js",
    "cli.js",
    "interpreter.js",
    "parser.js"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "brainfuck",
    "brainfuck interpreter",
    "esolangs"
  ],
  "dependencies": {
    "meow": "^3.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "xo": "^0.17.0"
  },
  "xo": {
    "esnext": true,
    "globals": [
      "afterEach",
      "beforeEach",
      "describe",
      "it"
    ],
    "rules": {
      "array-bracket-spacing": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always"
      ],
      "unicorn/explicit-length-check": 0,
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "no-self-compare": 1,
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "space": 2
  }
}
